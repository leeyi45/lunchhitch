generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model location {
  id     String   @id @default(auto()) @map("_id") @db.ObjectId
  name   String
  stores String[]
}

model user {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  email String
  name  String
}
