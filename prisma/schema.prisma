generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Location {
  id     String   @id @default(auto()) @map("_id") @db.ObjectId
  name   String
  stores String[]
}

model UserInfo {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String
  username String
}
