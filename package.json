{
  "name": "lunchhitch",
  "version": "0.1.0",
  "private": true,
  "scripts-info": {
    "//note": "Make sure the prisma client is generated before beginning development",
    "clean": "Delete cached files",
    "db:generate": "Update the prisma client based on the prisma schema",
    "dev": "Run the development server",
    "docs:server": "Run a local http server serving the docs folder"
  },
  "scripts": {
    "build": "next build",
    "clean": "rm -rf .next && yarn cache clean",
    "db:pull": "prisma db pull --force",
    "db:generate": "prisma generate",
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "docs:generate": "ts-node --skipProject docs/docs.ts",
    "docs:serve": "python docs/dev_server.py",
    "start": "next start",
    "lint": "next lint && eslint -c .eslintrc.js"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@firebase/auth": "^0.20.2",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.4",
    "@prisma/client": "^3.15.0",
    "dotenv": "^16.0.1",
    "firebase": "^9.8.2",
    "firebase-admin": "^11.0.0",
    "formik": "^2.2.9",
    "next": "12.1.6",
    "nookies": "^2.5.2",
    "prisma": "^3.15.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.1.6",
    "@types/node": "17.0.33",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.1.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "ts-node": "^10.8.1",
    "typedoc": "^0.22.17",
    "typescript": "4.6.4"
  }
}
